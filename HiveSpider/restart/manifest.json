{
    "manifest_version": 3,
    "name": "bilibili首页更新",
    "version": "0.0.3",
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "permissions": [ "cookies", "unlimitedStorage", "http://*/*", "https://*/*", "file:///*/*" ],
    "description": "extension description",
    "background": {
      "page": "background.html",
      "matches": ["<all_urls>"]},
    "content_scripts": [],
    "browser_action": {
        "default_icon": "static/z.jpg"
    }
}