{
  "name": "b站 header修改服务",
  "manifest_version": 3,
  "version": "0.0.1",
  "description": "fake request bee",
  "browser_action": {
    "default_icon": "batman.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "webRequestBlocking",
    "webRequest",
    "webNavigation",
    "*://*/*",
    "tabs",
    "notifications",
    "cookies",
    "unlimitedStorage",
    "http://*/*",
    "https://*/*",
    "file:///*/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "matches": [
    ]
  },
  "content_scripts": [
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}